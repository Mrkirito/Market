<div class="common_info">
	<h1 class="assured_info_title tbxz_title">投保选择</h1>
	<ul class="common-info-ul">
	<div class="main_insure_product_div" style="display: none;" data-pid="664" data-jbbe="$!product.jbbe" data-cpjc="倍享福两全" data-table="0">
		<li class="info-item main_line_height">
			<label class="info-name">保障期限</label>
			<select class="factor" name="bxnx">
				<option value="44">保障至80周岁</option>
				<option value="52">保障至70周岁</option>
				<option value="36">保障至60周岁</option>
				<option value="50">保障30年</option>
			</select>
			<span class="arrowr"></span>
		</li>
		<li class="info-item main_line_height">
			<label class="info-name">缴费期限</label>
			<select class="factor" name="jfnx">
				<option value="11">20年交</option>
				<option value="10">15年交</option>
				<option value="9">10年交</option>
			</select>
			<span class="arrowr"></span>
		</li>
		<li class="info-item main_line_height">
			<label class="info-name">倍享福</label>
			<input type="tel" placeholder="请输入保额" value="100000" data-pid="664" data-jbbe="$!product.jbbe" data-cpjc="倍享福两全" class="demo_price factor" name="be" maxlength="15"/>
			<i class="icon">元</i>
			<span class="error-tips" style="display:none;"></span>
		</li>
		<li class="info-item main_line_height">
			<input class="checkbox-single" type="checkbox" checked> 
			<label class="info-name">重疾</label>
			<dd class="single_li_dd">
			<input type="tel" placeholder="请输入保额" value="100000" data-pid="679" data-jbbe="$!product.jbbe" data-cpjc="重疾" class="demo_price factor" name="be" maxlength="15"/>
			<i class="icon">元</i>
			<span class="error-tips" style="display:none;"></span>
            </dd>
			
		</li>
		<li class="info-item main_line_height">
			<input class="checkbox-single" type="checkbox" checked> 
			<label class="info-name">意外</label>
			<dd class="single_li_dd">
			<input type="tel" placeholder="请输入保额" value="100000" data-pid="680" data-jbbe="$!product.jbbe" data-cpjc="意外" class="demo_price factor" name="be" maxlength="15"/>
			<i class="icon">元</i>
			<span class="error-tips" style="display:none;"></span>
            </dd>
		</li>
	</div>
	<li class="main_item" >
		<div class="creatDes_header">
			<span class="createDes-title verticalmiddle">
				<em>倍享福</em>			
			</span>
			<span class="creatDes-add-btn"> 
				<em class="add_icon"></em>
				<em class="add_font">添加主险</em>
			</span>
		</div>
	</li>
	<li class="main_item li_table_0" style="display:none">
		<div class="creatDes_header">
			<span class="createDes-title">
				<em>倍享福两全</em>
				<em class="price"></em>
			</span>
			<span class="createDes-btn">
				<a class="main_edit_btn" data-table="0">修改</a>
			</span>
		</div>
		<table class="createDes-table">
		<thead>
			<tr>
				<td>险种</td>
				<td>保额</td>
				<td>保费</td>
				<td>保障期限</td>
				<td>缴费期限</td>
			</tr>
		</thead>
		<tbody></tbody>
		</table>
	</li>
	#parse("product/addzx.vm")
	</ul>
</div>